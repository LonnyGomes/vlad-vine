{
  "name": "parse-photos",
  "version": "0.0.1",
  "description": "Parse images",
  "main": "dist/lib.js",
  "types": "dist/lib.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/lib.d.ts",
      "import": "./dist/lib.js"
    },
    "./types": {
      "types": "./dist/models.d.ts",
      "import": "./dist/models.js"
    },
    "./data": {
      "types": "./dist/data.d.ts",
      "import": "./dist/data.js"
    },
    "./shared": {
      "types": "./dist/shared.d.ts",
      "import": "./dist/shared.js"
    }
  },
  "files": [
    "dist",
    "images.json"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "start": "tsx src/index.ts",
    "dev": "tsx src/index.ts",
    "watch": "tsx watch src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Lonny Gomes",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^17.2.3",
    "emoji-flags": "^1.3.0",
    "exifr": "^7.1.3",
    "local-reverse-geocoder": "^0.16.7",
    "sharp": "^0.34.5"
  },
  "devDependencies": {
    "@types/geojson": "^7946.0.16",
    "@types/node": "^24.10.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
